    {
      "parameters": {
        "resource": "issue",
        "operation": "update",
        "issueKey": "={{ $('Check Status').item.json.key }}",
        "updateFields": {
          "statusId": "21"
        }
      },
      "id": "transition-in-progress",
      "name": "In Progress",
      "type": "n8n-nodes-base.jira",
      "typeVersion": 1,
      "position": [
        1500,
        300
      ],
      "credentials": {
        "jiraSoftwareCloudApi": {
          "id": "5iuB3iFLJn8tZPhM",
          "name": "Jira SW Cloud account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const issue = $('Check Status').item.json;\nconst prompt = `Auto-fix JIRA ticket\\n\\n**JIRA Key**: ${issue.key}\\n**Summary**: ${issue.fields?.summary || 'N/A'}`;\nreturn { json: { jira_key: issue.key, jira_summary: issue.fields?.summary, prompt } };"
      },
      "id": "build-prompt",
      "name": "Build Prompt",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1680,
        300
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "={{ $env.N8N_API_URL }}/v1/projects/system/chat",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({ user_message: $json.prompt, agent: 'Auto Fixer', source: 'scheduler', requester: 'auto-fix-scheduler', metadata: { jira_key: $json.jira_key, workflow_execution_id: $executionId } }) }}",
        "options": {
          "timeout": 600000
        }
      },
      "id": "auto-fix",
      "name": "Auto Fix",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        1860,
        300
      ],
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "jsCode": "const prev = $('Build Prompt').item.json;\nconst r = $input.item.json;\nlet res = { fixed: false, branch_name: '', commit_sha: '', files_changed: [], summary: '', error: '' };\nif (r.status !== 'completed') res.error = r.error?.message || 'API error';\nelse if (r.structured_output) res = { ...res, ...r.structured_output };\nelse res.error = 'No structured output';\n\n// CODEOWNERS ê¸°ë°˜ ë¦¬ë·°ì–´ ì„ íƒ\nconst REVIEWERS = {\n  frontend: ['suah.kang', 'yohan.shin', 'myunggyu.lee'],\n  backend: ['hyohyun.baek', 'hohwan.an', 'seungmoo.lee', 'junyeong.eom']\n};\nconst files = res.files_changed || [];\nconst hasFrontend = files.some(f => f.startsWith('front/'));\nconst hasBackend = files.some(f => f.startsWith('services/'));\nlet reviewers = [];\nif (hasFrontend) reviewers.push(REVIEWERS.frontend[0]);\nif (hasBackend) reviewers.push(REVIEWERS.backend[0]);\nif (reviewers.length === 0) reviewers.push(REVIEWERS.backend[0]);\n\nreturn { json: { ...prev, ...res, execution_id: r.id, reviewers } };"
      },
      "id": "parse-result",
      "name": "Parse Result",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2040,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $json.fixed }}",
              "rightValue": true,
              "operator": {
                "type": "boolean",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "is-fixed",
      "name": "Fixed?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        2220,
        300
      ]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "=https://gitlab.42dot.ai/api/v4/projects/tms%2Fappius/members/all?per_page=100",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpHeaderAuth",
        "options": {
          "timeout": 10000
        }
      },
      "id": "get-reviewers",
      "name": "Get Reviewers",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        2400,
        250
      ],
      "credentials": {
        "httpHeaderAuth": {
          "id": "QHpWT7FE12mKF6m9",
          "name": "Header Auth account"
        }
      },
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "jsCode": "const prev = $('Parse Result').item.json;\nconst members = $input.first().json || [];\nconst reviewerUsernames = prev.reviewers || [];\nconst reviewerIds = [];\nfor (const username of reviewerUsernames) {\n  const member = members.find(m => m.username === username);\n  if (member) reviewerIds.push(member.id);\n}\nreturn { json: { ...prev, reviewer_ids: reviewerIds } };"
      },
      "id": "map-reviewer-ids",
      "name": "Map Reviewer IDs",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2580,
        250
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://gitlab.42dot.ai/api/v4/projects/tms%2Fappius/merge_requests",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({ source_branch: $json.branch_name, target_branch: 'main', title: `[${$json.jira_key}] ${$json.jira_summary}`, description: `## Auto-fix\\n\\n${$json.summary}\\n\\n**Files changed:**\\n${($json.files_changed || []).map(f => '- ' + f).join('\\n')}\\n\\n---\\nðŸ¤– Generated by Auto Fixer`, remove_source_branch: true, reviewer_ids: $json.reviewer_ids }) }}",
        "options": {
          "timeout": 30000
        }
      },
