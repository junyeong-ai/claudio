          "duedate": "={{ $('Check Status').item.json.dueDate }}",
          "statusId": "51"
        }
      },
      "id": "transition-to-todo",
      "name": "To Do",
      "type": "n8n-nodes-base.jira",
      "typeVersion": 1,
      "position": [
        1320,
        250
      ],
      "credentials": {
        "jiraSoftwareCloudApi": {
          "id": "5iuB3iFLJn8tZPhM",
          "name": "Jira SW Cloud account"
